---
interface Props {
    title: string;
    number: string;
}

const { title, number } = Astro.props;
---

<div class="bg-natural-white rounded-lg p-4 text-center shadow-lg">
    <h3 class="text-xl font-bold mb-2">
        {title}
    </h3>
    <p class="text-4xl font-bold">{number}</p>
</div>
       

